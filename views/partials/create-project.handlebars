<section class="create-project-handler">
    <h2>Create a Project</h2>
    <div class="create-project-link">
        <input type="text" id="#create-title" placeholder="Project Title">
         <input type="date" id="#start-date">
          <input type="date" id="#end-date">
        <button type="submit" id="create-project-btn">
            <i class='far fa-arrow-alt-circle-right' class="create-proj-btn-partial">Create Project</i>
        </button>
    </div>
</section>
<main>
    <section class="project">
        <h2>Title</h2>
        <div class="project-dates">
            <p>Date Started: <span>1/10/21</span></p>
            <p>Date Ended: <span>2/21/21</span></p>
        </div>
        <h5><a href="/hours">Edit Hours</a></h5>
    </section>
</main>
<script src="/javascript/create-project.js"></script>